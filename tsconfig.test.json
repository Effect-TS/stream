{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "build/tsbuildinfo/test.tsbuildinfo",
    "rootDir": "./",
    "noEmit": true,
    "types": [
      "vitest/globals",
    ],
    "paths": {
      "@effect/io/*": [
        "./node_modules/@effect/io/src/*"
      ],
      "@effect/stream": [
        "./src/index.ts"
      ],
      "@effect/stream/test/*": [
        "./test/*"
      ],
      "@effect/stream/examples/*": [
        "./examples/*"
      ],
      "@effect/stream/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "test/**/*.ts",
    "src/**/*.ts"
  ]
}
